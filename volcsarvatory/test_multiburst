from volcsarvatory import prepare_multibursts as pm

def test_split_multiburst():
    multiburst_dict = {
        "173_370304": ("IW1", "IW2", "IW3"),
        "173_370305": ("IW1", "IW2", "IW3"),
        "173_370306": ("IW1", "IW2", "IW3"),
        "173_370307": ("IW1", "IW2", "IW3")
    }
    assert len(pm.split_multiburst(multiburst_dict)) == 1

def test_fill_holes():    
    multiburst_dict = {
        "173_370304": ("IW1", "IW3"),
        "173_370305": ("IW1", "IW2", "IW3"),
        "173_370306": ("IW1", "IW2", "IW3"),
        "173_370307": ("IW1", "IW3")
    }
